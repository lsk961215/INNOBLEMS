<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="skill">
	
	<insert id="addUserSkill" parameterType="com.spring.innoblems.dto.SkillDTO">
			INSERT INTO 
				USER_SKILL_TABLE (
					USR_SEQ, 
					SK_CD
					)
			VALUES	(
				#{usrSeq}, 
				#{skill}
				)
	</insert>
	
</mapper>